<h1>Friends List goes here</h1>

<%friends.forEach(function(friend){%>
    <li>
        My friends' name is <%= friend%>
    </li>
<%});%>

<form action="/addfriends" method="POST">
    <input type="text" placeholder="Please enter name">
    <button>Add new friend</button>
</form>

<!-- When you are done with the form, you'll be redirected to the webpage that you
wrote on action.-->